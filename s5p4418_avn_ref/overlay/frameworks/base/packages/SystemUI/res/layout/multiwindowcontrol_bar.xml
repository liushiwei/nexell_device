<com.android.systemui.statusbar.avn.MultiWindowControlBarView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:systemui="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:background="@drawable/multiwindow_controlbar_background"
    >

    <FrameLayout android:id="@+id/rot0"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        >

        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:id="@+id/multiwin_buttons"
            android:animateLayoutChanges="true"
            >
            <com.android.systemui.statusbar.policy.KeyButtonView android:id="@+id/multiwin_control_leftfull"
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:src="@drawable/ic_sysbar_multiwindow_leftfull"
                systemui:keyCode="96"
                android:layout_weight="0"
                android:scaleType="center"
                android:contentDescription="@string/accessibility_multiwin_leftfull"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="32px"
                android:layout_weight="1"
                android:visibility="invisible"
                />
            <com.android.systemui.statusbar.policy.KeyButtonView android:id="@+id/multiwin_control_rightfull"
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:src="@drawable/ic_sysbar_multiwindow_rightfull"
                systemui:keyCode="97"
                android:layout_weight="0"
                android:scaleType="center"
                android:contentDescription="@string/accessibility_multiwin_rightfull"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="32px"
                android:layout_weight="1"
                android:visibility="invisible"
                />
            <com.android.systemui.statusbar.policy.KeyButtonView android:id="@+id/minilauncher"
                android:layout_width="match_parent"
                android:layout_height="64px"
                android:src="@drawable/ic_sysbar_launcher"
                systemui:keyCode="131"
                android:layout_weight="0"
                android:scaleType="center"
                android:contentDescription="@string/accessibility_minilauncher"
                />
        </LinearLayout>

    </FrameLayout>

</com.android.systemui.statusbar.avn.MultiWindowControlBarView>
